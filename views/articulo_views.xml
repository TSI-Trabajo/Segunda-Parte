<odoo>
  <record id="upobarber_articulo_form_view" model="ir.ui.view">
    <field name="name">upobarber.articulo.form</field>
    <field name="model">upobarber.articulo</field>
    <field name="arch" type="xml">
      <form string="Articulo">
        <sheet layout="auto">
          <group>
            <field name="name" />
            <field name="marca" />
            <field name="modelo" />
            <field name="precio" />
            <field name="stock" />
            <field name="photo" />
            <field name="producto_id" widget="many2one" />
            <!--<field name="users_ids">
              <tree>
                <field name="name" />
                <field name="idcard" />
              </tree>
            </field>-->
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="upobarber_articulo_tree_view" model="ir.ui.view">
    <field name="name">upobarber.articulo.tree</field>
    <field name="model">upobarber.articulo</field>
    <field name="arch" type="xml">
      <tree string="tree_string">
        <field name="name" />
            <field name="marca" />
            <field name="modelo" />
            <field name="precio" />
            <field name="stock" />
            <field name="photo" />
            <field name="producto_id" widget="many2one" />
      </tree>
    </field>
  </record>

