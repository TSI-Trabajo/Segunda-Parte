<odoo>
  <record id="upobarber_horario_form_view" model="ir.ui.view">
    <field name="name">upobarber.horario.form</field>
    <field name="model">upobarber.horario</field>
    <field name="arch" type="xml">
      <form string="Empleado">
        <sheet layout="auto">
          <group>
            <field name="nombre" />
            <field name="apellidos" />
            <field name="telefono" />
            <field name="correoElectronico" />
            <field name="numeroCuenta" />
            <field name="id_Horario" widget="many2one" />
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="upobarber_horario_tree_view" model="ir.ui.view">
    <field name="name">upobarber.horario.tree</field>
    <field name="model">upobarber.horario</field>
    <field name="arch" type="xml">
      <tree string="tree_string">
            <field name="nombre" />
            <field name="apellidos" />
            <field name="telefono" />
            <field name="correoElectronico" />
            <field name="numeroCuenta" />
            <field name="id_Horario" widget="many2one" />
      </tree>
    </field>
  </record>
</odoo>